	local AimModeDropdown = AimbotSection:AddDropdown("AimMode", {Title="Aim Mode",Description="Changes the Aim Mode",Values={"Camera"},Default=Configuration.AimMode,Callback=function(Value)
		Configuration.AimMode = Value;
	end});
